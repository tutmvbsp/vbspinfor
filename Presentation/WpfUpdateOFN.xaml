        <Label Content="Ngày GD Offline" Height="auto" HorizontalAlignment="Left" Margin="16,12,0,0" Name="lblNgay" VerticalAlignment="Top" Width="auto" />
        <Label Content="Mã điểm GD" Height="auto" HorizontalAlignment="Left" Margin="261,12,0,0" Name="lblPoint" VerticalAlignment="Top" Width="auto" />
        <ComboBox Height="21" HorizontalAlignment="Left" Margin="543,15,0,0" Name="CboTable" VerticalAlignment="Top" Width="132" SelectionChanged="CboTable_SelectionChanged" />
        <ComboBox Height="21" HorizontalAlignment="Left" Margin="392,15,0,0" Name="CboTranCd" VerticalAlignment="Top" Width="132"  />
    </Grid>
</Window>
