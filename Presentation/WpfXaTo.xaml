                <RadioButton Content="Năm" Height="auto" HorizontalAlignment="Left" Margin="6,57,0,0" Name="radioButton3" VerticalAlignment="Top" Width="auto" />
            </Grid>
        </GroupBox>
        <GroupBox Header="Mẩu" Height="106" HorizontalAlignment="Left" Margin="362,13,0,0" Name="groupBox2" VerticalAlignment="Top" Width="102">
            <Grid>
                <RadioButton Content="Chi Tiết" Height="auto" HorizontalAlignment="Left" Margin="6,18,0,0" Name="radioButton4" VerticalAlignment="Top" Width="auto" IsChecked="True" />
                <RadioButton Content="Tổng Hơp" Height="auto" HorizontalAlignment="Left" Margin="6,50,0,0" Name="radioButton5" VerticalAlignment="Top" Width="auto" />
            </Grid>
        </GroupBox>

    </Grid>
</Window>
