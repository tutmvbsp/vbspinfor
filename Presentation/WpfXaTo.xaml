        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Số liệu theo xã / tổ " Height="222" Width="501.36" WindowStartupLocation ="CenterScreen" Icon="/Icons/officemac01.ico" Loaded="Window_Loaded">
