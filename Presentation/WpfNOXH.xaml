        <Label Content="Ngày số liệu" Height="29" HorizontalAlignment="Left" Margin="43,14,0,0" Name="lblNgay" VerticalAlignment="Top" Width="auto" />
        <Label Content="POS" Height="23" HorizontalAlignment="Left" Margin="53,44,0,0" Name="lblPos" VerticalAlignment="Top" Width="auto" />
        <telerik:RadComboBox Height="22" HorizontalAlignment="Left" Margin="121,51,0,0" Name="CboPos" VerticalAlignment="Top" Width="106" SelectionChanged="CboPos_SelectionChanged"  />
        <telerik:RadComboBox Height="22" HorizontalAlignment="Left" Margin="121,73,0,0" Name="CboXa" VerticalAlignment="Top" Width="106" />
