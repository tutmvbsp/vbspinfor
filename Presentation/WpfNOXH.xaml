        <Button Content="OK" Height="22" HorizontalAlignment="Left" Margin="92,113,0,0" Name="btnOk" VerticalAlignment="Top" Width="91" Click="btnOk_Click" />
        <GroupBox Header="Export" Height="85" HorizontalAlignment="Left" Margin="271,10,0,0" Name="GrpIn" VerticalAlignment="Top" Width="93">
