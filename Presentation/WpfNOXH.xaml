                <RadioButton GroupName="In" Content="In" Height="20" HorizontalAlignment="Left" Margin="9,4,0,0" Name="Ration1" VerticalAlignment="Top" Width="auto" IsChecked="True" />
                <RadioButton GroupName="In" Content="Excel" Height="23" HorizontalAlignment="Left" Margin="9,25,0,0" Name="Ration2" VerticalAlignment="Top" Width="auto" />
            </Grid>
        </GroupBox>

    </Grid>
</Window>
