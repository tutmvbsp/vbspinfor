        xmlns:telerik="http://schemas.telerik.com/2008/xaml/presentation"
        Title="Phân tích số liệu nhà ở xã hội " Height="190" Width="402.924" WindowStartupLocation =" CenterScreen" Loaded="Window_Loaded" Icon="Icons/Network_b.ico">
